#Export the encrypted private key from PFX file
.\openssl pkcs12 -in difchealth.pfx -nocerts -out key.pem

#Remove encryption from private key using the password with OpenSSL
./openssl rsa -in key.pem -out key2.pem

#Create an SSL secret using the private key and CRT file
kubectl create secret tls aks-difc-health-ingress-tls \
    --namespace difc-health \
    --key ./difc-healthscheme_com.key \
    --cert ./difc-healthscheme_com.crt